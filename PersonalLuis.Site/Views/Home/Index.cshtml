@using PersonalLuis.Site.Models.Enums;
@model HomeVm

<!-- About Section start -->
<section id="about" class="scroll-section root-sec padd-tb-60 grey lighten-5 about-wrap">
  <div class="container">
    <div class="row">
      <div class="clearfix about-inner">

        <div class="col-sm-12 col-md-4">
          <div class="person-about">
            <h3 class="about-subtitle">My Story</h3>
            <p>@Html.Raw(Model.GeneralInfo.History)</p>
            <a class="waves-effect waves-light btn-large brand-bg white-text"><i class="mdi-content-archive left"></i> Download Resume</a>
          </div>
        </div>
        <!-- about me description -->

        <div class="col-sm-6 col-md-4">
          <div class="person-img wow fadeIn">
            <img class="z-depth-1" src="@Model.GeneralInfo.PictureUrl" alt="luis-palacios">
          </div>
        </div>
        <!-- about me image -->

        <div class="col-sm-6 col-md-4">
          <div class="person-info">
            <h3 class="about-subtitle">Personal Information</h3>
            <h5><span>Name :</span> @Model.GeneralInfo.Name</h5>
            <h5><span>Age :</span> @Model.GeneralInfo.Age</h5>
            <h5><span>Phone :</span> +0123456789</h5>
            <h5><span>Email :</span> @Model.GeneralInfo.Email</h5>
            <h5><span>Address :</span> @Model.GeneralInfo.Address</h5>
          </div>

          <div class="about-social">
            <ul>
              @foreach(var socialMediaUrl in @Model.SocialMediaUrls)
              {
                switch(socialMediaUrl.SocialMediaType)
                {
                  case SocialMediaType.Facebook:
                  <li>
                    <a href="@socialMediaUrl.Url" target="_blank" class="btn-floating waves-effect waves-light white"><i class="fa fa-facebook"></i></a>
                  </li>
                  break;
                  case SocialMediaType.Twitter:
                  <li>
                    <a href="@socialMediaUrl.Url" target="_blank" class="btn-floating waves-effect waves-light white"><i class="fa fa-twitter"></i></a>
                  </li>
                  break;
                  case SocialMediaType.GooglePlus:
                  <li>
                    <a href="@socialMediaUrl.Url" target="_blank" class="btn-floating waves-effect waves-light white"><i class="fa fa-google-plus"></i></a>
                  </li>
                  break;
                  case SocialMediaType.LinkedIn:
                  <li>
                    <a href="@socialMediaUrl.Url" target="_blank" class="btn-floating waves-effect waves-light white"><i class="fa fa-linkedin"></i></a>
                  </li>
                  break;
                  case SocialMediaType.Github:
                  <li>
                    <a href="@socialMediaUrl.Url" target="_blank" class="btn-floating waves-effect waves-light white"><i class="fa fa-github"></i></a>
                  </li>
                  break;
                }
              }
            </ul>
          </div>
        </div>
        <!-- about me info -->

      </div>
    </div>
  </div>
  <!-- .container end -->
</section>
<!-- #about Section end -->

<!-- Resume Section start -->
<section id="resume" class="scroll-section ">
  <section id="skill" class="root-sec white skill-wrap">
  <div class="sec-overlay">
    <div class="container">
      <div class="row">
        <div class="clearfix skill-inner">
          <div class="col-sm-12 col-md-3">
            <div class="skill-left">
              <h2 class="title">Languages</h2>
              <p class="regular-text">These are some of the main Languages I've used for software development</p>
            </div>
