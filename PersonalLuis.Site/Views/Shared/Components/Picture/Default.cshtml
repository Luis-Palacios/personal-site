@model PictureComponentVm

<picture class="lazy">
    @foreach (var imageUrl in Model.ImageUrls)
    {
        <source data-srcset="@imageUrl" type="image/@imageUrl.GetExtension()" />
    }
    <img data-src="@Model.ImageUrls.LastOrDefault()" src="/images/blur.png" alt="@Model.ImageAlt" class="@Model.ImageClass"/>
</picture>

